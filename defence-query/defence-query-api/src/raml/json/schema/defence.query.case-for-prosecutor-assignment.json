  "type": "object",
  "properties": {
    "caseURN": {
      "type": "string"
    },
    "caseId": {
      "$ref": "http://justice.gov.uk/domain/core/common/definitions.json#/definitions/uuid"
    },
    "error": {
      "$ref": "http://justice.gov.uk/cps/defence/error.json"
    }
  },
  "oneOf": [
    {
      "required": [
        "caseURN",
        "caseId"
      ]
    },
    {
      "required": [
        "error"
      ]
    }
  ]
}