{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "http://moj.gov.uk/cpp/defence/prosecutor.json",
  "type": "object",
  "description": "Prosecutor Details",
  "properties": {
    "prosecutingAuthority": {
      "type": "string",
